@page "/"
@namespace NanoSoftGreenMark.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>NanoSoftGreenMark</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="css/site.css" rel="stylesheet" />

    <link href="~/css/Design.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.12.0/css/all.css">
    <link href="~/css/Model.css" rel="stylesheet" />
    <link href="~/css/VbarIcon.css" rel="stylesheet" />
    <link href="~/css/CardView.css" rel="stylesheet" />
    <link href="~/css/tablestyle.css" rel="stylesheet" />
    <link href="~/css/PurchaseVeiw.css" rel="stylesheet" />
    <link href="~/Sale.css" rel="stylesheet" />
  
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <div id="blazor-error-ui">
        <environment include="Staging,Production">
            An error has occurred. This application may no longer respond until reloaded.
        </environment>
        <environment include="Development">
            An unhandled exception has occurred. See browser dev tools for details.
        </environment>
        <a href="" class="reload">Reload</a>
        <a class="dismiss">🗙</a>
    </div>

    <script src="_framework/blazor.server.js"></script>
    <script src="~/css/Custom.js"></script>
    <script src="_content/BlazorInputFile/inputfile.js"></script>
</body>
</html>
